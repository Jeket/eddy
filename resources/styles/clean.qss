/**************************************************************************/
/*                                COMBO BOX                               */
/**************************************************************************/
QComboBox {
background: #F2F2F2;
border: 1px solid #BABABA;
padding: 1px 18px 1px 3px;
selection-background-color: #D0D0D0;
selection-color: #000000;
}
QComboBox:editable {
background: white;
}
QComboBox:!editable,
QComboBox::drop-down:editable {
 background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                            stop: 0.0 #E0E0E0, stop: 0.3 #DCDCDC,
                            stop: 0.7 #DBDBDB, stop: 1.0 #D3D3D3);
}
QComboBox:!editable:on,
QComboBox::drop-down:editable:on {
background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                            stop: 0.0 #F8F8F8, stop: 0.3 #F4F4F4,
                            stop: 0.7 #F0F0F0, stop: 1.0 #EDEDED);
}
QComboBox::drop-down {
subcontrol-origin: padding;
subcontrol-position: top right;
border-left: 1px solid #BABABA;
}
QComboBox::down-arrow {
image: url(:/icons/18/arrow-drop-down);
}
QComboBox QAbstractItemView {
background: #F2F2F2;
}
/**************************************************************************/
/*                              RADIO BUTTONS                             */
/**************************************************************************/
QRadioButton {
spacing: 4px;
}
QRadioButton::indicator {
height: 14px;
width: 14px;
}
QRadioButton::indicator::unchecked {
image: url(:/icons/24/radio-outline);
}
QRadioButton::indicator::checked {
image: url(:/icons/24/radio-checked);
}
/**************************************************************************/
/*                                TREE VIEW                               */
/**************************************************************************/
QTreeView::item {
border: 0;
}
QTreeView::item:hover {
border: 0;
}
QTreeView::item:selected {
background: #4FC3F7;
color: #000000;
}
QTreeView::item:!selected {
background: #FFFFFF;
color: #000000;
}
QTreeView::branch:selected {
background: #4FC3F7;
}
QTreeView::branch:!selected {
background: #FFFFFF;
}
QTreeView::branch:has-siblings:!adjoins-item {
border-image: url(:/icons/treeview/branch-vline) 0;
}
QTreeView::branch:has-siblings:adjoins-item {
border-image: url(:/icons/treeview/branch-more) 0;
}
QTreeView::branch:!has-children:!has-siblings:adjoins-item {
border-image: url(:/icons/treeview/branch-end) 0;
}
QTreeView::branch:has-children:!has-siblings:closed,
QTreeView::branch:closed:has-children:has-siblings {
border-image: none;
image: url(:/icons/treeview/branch-closed);
}
QTreeView::branch:open:has-children:!has-siblings,
QTreeView::branch:open:has-children:has-siblings  {
border-image: none;
image: url(:/icons/treeview/branch-open);
}
/**************************************************************************/
/*                               STATUS BAR                               */
/**************************************************************************/
QStatusBar {
background: #BBBBBB;
}
/**************************************************************************/
/*                                 MISC                                   */
/**************************************************************************/
QLineEdit {
background: #FFFFFF;
}
QMessageBox QLabel {
font-weight: normal;
}
/**************************************************************************/
/*                            ERROR CAPTION                               */
/**************************************************************************/
QLabel.invalid {
color: #EC3F3A;
}